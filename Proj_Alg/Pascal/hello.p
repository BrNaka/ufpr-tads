(* Program to demonstrate Pascal compilation *)

program Hello ;
var a, b, c: Integer;
begin
  write('Por favor insira o valor da variável a');
  readln(a);
  write('Por favor insira o valor da variável b');
  readln(b);
  write('Por favor insira o valor da variável c');
  readln(c);
  
  writeln('Valor da varíavel a, b, c = ', a, b, c, 'bruno');
end.
